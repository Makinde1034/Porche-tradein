  "description": "",
  "main": "Utils.js",
  "scripts": {
    "lint": "npm run jslint && npm run stylelint",
    "jslint": "eslint --config .eslintrc.json --ext .js,.vue ./",
    "stylelint": "stylelint ./**/*.scss --config ./stylelint.config.js",
    "format:check": "prettier --check \"**/*.{vue,js,json,scss,html}\"",
    "format:write": "eslint --fix --config .eslintrc.json --ext .js --ext .vue ./ && prettier --write \"**/*.{vue,js,json,scss,html}\""
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.16.5",
    "@babel/eslint-parser": "^7.16.5",
    "@babel/plugin-proposal-class-properties": "^7.16.5",
    "@babel/plugin-proposal-decorators": "^7.16.5",
    "@babel/preset-env": "^7.16.5",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-private-props": "^0.3.0",
    "eslint-plugin-vue": "^8.2.0",
    "eslint": "^8.4.1",
    "prettier": "^2.5.1",
    "stylelint": "14.1.0",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-standard-scss": "^3.0.0",
    "stylelint-config-standard": "^24.0.0",
    "stylelint-prettier": "^2.0.0",
    "stylelint-selector-bem-pattern": "^2.1.1"
  },
